<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>AutoFixture</groupId>
  <artifactId>AutoFixture</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>AutoFixtureGenerator</name>
  <dependencies>
    <dependency>
      <scope>system</scope>
      <groupId>group-a</groupId>
      <artifactId>guava</artifactId>
      <version>16.0.1</version>
      <systemPath>${basedir}/Lib/guava-16.0.1.jar</systemPath>
    </dependency>
    <dependency>
      <scope>system</scope>
      <groupId>group-a</groupId>
      <artifactId>junit</artifactId>
      <version>4.11</version>
      <systemPath>${basedir}/Lib/junit-4.11.jar</systemPath>
    </dependency>
    <dependency>
      <scope>system</scope>
      <groupId>group-a</groupId>
      <artifactId>hamcrest-core</artifactId>
      <version>1.3</version>
      <systemPath>${basedir}/Lib/hamcrest-core-1.3.jar</systemPath>
    </dependency>
    <dependency>
      <scope>system</scope>
      <groupId>group-a</groupId>
      <artifactId>jmock</artifactId>
      <version>2.6.0</version>
      <systemPath>${basedir}/Lib/jmock-2.6.0/jmock-2.6.0.jar</systemPath>
    </dependency>
    
  </dependencies>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>